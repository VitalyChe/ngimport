{
  "name": "ngimport",
  "version": "0.5.2",
  "description": "ES6 imports for $http, $log, and other Angular 1 services",
  "main": "./ngimport.js",
  "main:esnext": "./ngimport.es2015.js",
  "typings": "./ngimport.d.ts",
  "scripts": {
    "build": "tsc -d -m es2015 -t es5 --outDir . && mv ./ngimport.js ./ngimport.es2015.js && tsc -m commonjs -t es5 --outDir .;",
    "watch": "tsc -w",
    "test": "karma start --single-run",
    "tdd": "karma start"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/bcherny/ngimport.git"
  },
  "keywords": [
    "import",
    "angular",
    "require",
    "dependency",
    "inject",
    "di",
    "typescript",
    "es6"
  ],
  "author": "Boris Cherny <boris@performancejs.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bcherny/ngimport/issues"
  },
  "homepage": "https://github.com/bcherny/ngimport#readme",
  "devDependencies": {
    "@types/angular-mocks": "^1.5.9",
    "@types/jasmine": "^2.5.43",
    "angular-mocks": "^1.6.2",
    "browserify": "^14.1.0",
    "jasmine": "^2.5.3",
    "karma": "^1.4.1",
    "karma-browserify": "^5.1.1",
    "karma-chrome-launcher": "^2.0.0",
    "karma-jasmine": "^1.1.0",
    "karma-mocha-reporter": "^2.2.2",
    "karma-source-map-support": "^1.2.0",
    "rollupify": "^0.3.9",
    "typescript": "^2.1.6",
    "watchify": "^3.9.0"
  },
  "dependencies": {
    "@types/angular": "^1.6.5",
    "angular": "^1.6.2"
  }
}
