{
  "name": "ngimport",
  "version": "0.2.2",
  "description": "Finally, ES2015 imports for Angular 1 services!",
  "main": "./ngimport.js",
  "main:esnext": "./ngimport.es2015.js",
  "typings": "./ngimport.d.ts",
  "scripts": {
    "build": "tsc -d -m es2015 --outDir .; mv ./ngimport.js ./ngimport.es2015.js; tsc -m commonjs --outDir .;",
    "watch": "tsc -w",
    "test": "karma start --log-level debug --single-run",
    "tdd": "karma start",
    "postinstall": "typings install"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/bcherny/ngimport.git"
  },
  "keywords": [
    "import",
    "angular",
    "require",
    "dependency",
    "inject",
    "di"
  ],
  "author": "Boris Cherny <boris@performancejs.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bcherny/ngimport/issues"
  },
  "homepage": "https://github.com/bcherny/ngimport#readme",
  "devDependencies": {
    "angular-mocks": "^1.5.5",
    "browserify": "^13.0.1",
    "jasmine": "^2.4.1",
    "jasmine-core": "^2.4.1",
    "karma": "^0.13.22",
    "karma-browserify": "^5.0.5",
    "karma-chrome-launcher": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-mocha-reporter": "^2.0.3",
    "karma-source-map-support": "^1.1.0",
    "rollupify": "^0.3.0"
  },
  "dependencies": {
    "angular": "^1.5.5",
    "typings": "^1.0.4"
  }
}
